#include <iostream>

using namespace std;
int x
bool q(x){
    if( x == 2) return true;
    if( (x==0) || (x%2 == 0) ) return false;
    if ( x == 1 ) return false;
    int k = x / 2;
    for( int i = 3; i <= k; i++)
       if( x % i == 0) return false;
          return true;
}
